#!/usr/bin/env ruby
require 'bundler/setup'
require 'fepath'
require 'pry'
require 'fileutils'
require 'open-uri'
require 'active_support/core_ext'
require 'active_support/inflector'
require 'tty'


data = JSON.parse File.read('tmp/my_paths.json')

data.each do |path|
  PathConverter.new(path).to_dir(dest: 'tmp/tiyo_course')
end
